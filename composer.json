{
    "name": "jrean/laravel-supercollege-browse",
    "description": "SuperCollege Browse API wrapper for Laravel 5.*",
    "keywords": ["laravel", "framework", "supercollege", "scholarships", "scholarship", "school", "schools", "student", "students"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jean Ragouin",
            "email": "go@askjong.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.4.*|5.5.*|5.6.*"
    },
    "autoload": {
        "psr-4": {
            "Jrean\\SuperCollegeBrowse\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "fzaninotto/faker": "^1.6",
        "mockery/mockery": "^0.9.5",
        "satooshi/php-coveralls": "^1.0",
        "friendsofphp/php-cs-fixer": "^1.12",
        "sllh/php-cs-fixer-styleci-bridge": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jrean\\SuperCollegeBrowse\\SuperCollegeBrowseServiceProvider"
            ],
            "aliases": {
                "SuperCollege": "Jrean\\SuperCollegeBrowse\\Facades\\SuperCollegeBrowse"
            }
        }
    }
}
